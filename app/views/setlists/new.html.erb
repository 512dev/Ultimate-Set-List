<%= simple_form_for @setlist do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :artist %>
  </div>
<%= f.input :user_id, :as => :hidden, :input_html => { :value => current_user.id } %>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
